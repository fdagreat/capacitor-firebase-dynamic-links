{
  "name": "@fdagreat/capacitor-firebase-dynamic-links",
  "version": "1.0.0",
  "description": "Capacitor Plugin for Firebase Dynamic Links",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && npm run docgen",
    "clean": "rimraf -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "format:write": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "docgen": "docgen --api CapacitorFirebaseDynamicLinksPlugin --output-readme README.md --output-json dist/docs.json"
  },
  "author": "Fredy Mgimba (fredgerman30@gmail.com)",
  "license": "MIT",
  "peerDependencies": {
    "@capacitor/core": "^4.0.0"
  },
  "devDependencies": {
    "@capacitor/android": "^4.0.0",
    "@capacitor/docgen": "^0.2.0",
    "@capacitor/ios": "^4.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "FdagreatCapacitorFirebaseDynamicLinks.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fdagreat/capacitor-firebase-dynamic-links"
  },
  "bugs": {
    "url": "https://github.com/fdagreat/capacitor-firebase-dynamic-links/issues"
  },
  "homepage": "https://github.com/fdagreat/capacitor-firebase-dynamic-links#readme"
}
